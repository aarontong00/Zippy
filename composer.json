{
    "name": "Aarontong00/zippy",
    "type": "library",
    "description": "Zippy, the archive manager companion just for  php 5.6 fork",
    "keywords": ["zip", "tar", "bzip", "compression"],
    "license": "MIT",
    "authors": [
        {
            "name": "aarontong00",
            "email": "2845652696@qq.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "doctrine/collections": "~1.0",
        "symfony/filesystem": "^2.0.5 || ^3.0",
        "symfony/process": "^2.1 || ^3.0 || ^4.0",
        "symfony/polyfill-mbstring": "^1.3"
    },
    "require-dev": {
        "ext-zip": "*",
        "guzzle/guzzle": "~3.0",
        "guzzlehttp/guzzle": "^6.0",
        "phpunit/phpunit": "^4.0 || ^5.0",
        "symfony/finder": "^2.0.5 || ^3.0 || ^4.0"
    },
    "suggest": {
        "ext-zip": "To use the ZipExtensionAdapter",
        "guzzlehttp/guzzle": "To use the GuzzleTeleporter with Guzzle 6",
        "guzzle/guzzle": "To use the GuzzleTeleporter with Guzzle 3"
    },
    "autoload": {
        "psr-4": {
            "Aarontong00\\Zippy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aarontong00\\Zippy\\Functional\\": "tests/Functional/",
            "Aarontong00\\Zippy\\Tests\\": "tests/Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    }
}
